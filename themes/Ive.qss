QAbstractItemView {
	border: 0px solid red;
}

QAbstractScrollArea {
	border: 0px solid red;
}

QTableView {
	background-color: rgb(100,100,100);
	gridline-color: rgb(70, 65, 65);
}


QHeaderView {
	background: solid rgb(100,100,100);
}

QHeaderView::section {
	background: solid rgb(120,120,120);
}

QHeaderView::down-arrow {
	/*image: url(D:/erdem/coding/Flow/resources/Laminar/icons/ic_keyboard_arrow_down_white_48dp_2x.png);*/
	width: 24px;
	height: 24px;
}

QHeaderView::up-arrow {
	/*image: url(D:/erdem/coding/Flow/resources/Laminar/icons/ic_keyboard_arrow_up_white_48dp_2x.png);*/
	width: 24px;
	height: 24px;
}

QTableWidget {
	background-color: rgb(100,100,100);
	gridline-color: rgb(70, 65, 65);
	border: 0px;
}

QScrollBar:vertical {
	border-left: 2px solid rgb(225,150, 0);
	background-color: rgb(100, 95, 95);
	width: 19px;
	margin: 16px 0 16px 0;
}

QScrollBar::handle:vertical {
	min-height: 16px;
	border: 0px;
	background-color: rgb(125, 130, 125);
	/*image: url(icons/baseline_drag_indicator_white_cropped_vertical.png);*/
}

QScrollBar::handle:vertical:hover:!pressed {
	background-color: rgb(120, 124, 120);
}

QScrollBar::handle:vertical:pressed {
	background-color: rgb(112, 116, 112);
}

QScrollBar::add-line:vertical {
	border-left: 2px solid rgb(225,150, 0);
	background-color: rgb(100, 95, 95);
    height: 16px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
	border-left: 2px solid rgb(225,150, 0);
	background-color: rgb(100, 95, 95);
    height: 16px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical{
	image: url(icons/ic_keyboard_arrow_up_white_48dp_2x.png);
	width: 24px;
	height: 24px;
}

QScrollBar::down-arrow:vertical{
	image: url(icons/ic_keyboard_arrow_down_white_48dp_2x.png);
	width: 24px;
	height: 24px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}

QScrollBar:horizontal {
	border-top: 2px solid rgb(225,150, 0);
	background-color: rgb(100, 95, 95);
	height: 19px;
	margin: 0px 16px 0px 16px;
}

QScrollBar::handle:horizontal:hover:!pressed {
	background-color: rgb(120, 124, 120);
}

QScrollBar::handle:horizontal:pressed {
	background-color: rgb(112, 116, 112);
}

QScrollBar::handle:horizontal {
	min-width: 16px;
	border: 0px;
	background-color: rgb(125, 130, 125);
	image: url(baseline_drag_indicator_white_cropped_horizontal.png);
}

QScrollBar::add-line:horizontal {
	border-top: 2px solid rgb(225,150, 0);
	background-color: rgb(100, 95, 95);
    width: 16px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
	border-top: 2px solid rgb(225,150, 0);
	background-color: rgb(100, 95, 95);
    width: 16px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::left-arrow:horizontal{
	image: url(icons/ic_keyboard_arrow_left_white_48dp_2x.png);
	width: 24px;
	height: 24px;
}

QScrollBar::right-arrow:horizontal{
	image: url(icons/ic_keyboard_arrow_right_white_48dp_2x.png);
	width: 24px;
	height: 24px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}

QSpinBox {
	background-color: rgb(100, 100, 100);
	border: none;
}


QSpinBox::up-button {
    subcontrol-origin: border;
	subcontrol-position: top right;
	border: none;
}

QSpinBox::up-arrow {
	image: url(icons/ic_keyboard_arrow_up_white_48dp_2x.png);
	height: 16px;
	width: 16px;
}

QSpinBox::down-button {
    subcontrol-origin: border;
	subcontrol-position: bottom right;
	border: none;
}

QSpinBox::down-arrow {
	image: url(icons/ic_keyboard_arrow_down_white_48dp_2x.png);
	height: 16px;
	width: 16px;
}

