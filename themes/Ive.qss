* {
	font-size: 14px;
	color: rgb(245,245,245);
	border: none;
}

QMenu{
	background: rgb(85, 85, 85);
}

QMenu::item:selected{
	background-color: rgb(225, 125, 20);
}

QMenu::item:disabled{
	color: rgb(45, 45, 60);
}
QMenu::item{
	color: rgb(245,245,245);
}

QMainWindow {
	background-color: rgb(50, 50, 50);
	/*background-image: url(:ui/bg4.png);*/
	border: none;
}

QWidget[erdem=true] {
	background: rgb(255, 43, 43);
}

QWidget[toolbar=true] {
	background: rgb(80, 75, 75);
}

QWidget[divider=true] {
	background-color: rgb(80, 75, 75);
}


QPushButton[background=true] {
	background: rgb(100, 95, 95);
	border: 0px;
}

QPushButton[divider=true] {
	background: rgb(80, 60, 30);
	border: 0px;
}

QToolTip
{
    background-color: rgb(45, 45, 45);
    color: white;
    border: 1px solid rgb(45, 45, 45);

}
QPushButton[right=true] {
	background: rgb(80, 60, 30);
	border: 0px;
}

QPushButton[left=true] {
	background: rgb(100, 70, 40);
	border: 0px;
}

QLabel {
	color: rgb(128,128,128);
}

QLabel:disabled {
	color: rgb(80,80,80);
}

QLabel[header=true] {
	/*font: arial;*/
	font-size: 16px;
	color: rgb(250,175, 0);
}

QLabel[footer=true] {
	/*font: arial;*/
	font-size: 12px;
	color: rgb(225, 200, 200);
}

QLabel[title=true] {
	/*font: arial;*/
	font-size: 16px;
	color: rgb(0,175, 250);
}

LogWidget {
	background: solid #333;
	border-style: solid;
	border: 1px inset #202020;
/*	border-radius: 1;*/
	background-clip: border;
	padding: 0px;
	font: arial;
	font-size: 12px;
}

QLCDNumber {
	color: rgb(247, 147, 30);
}

QCheckBox::indicator::checked {
	color: rgb(100, 247, 30);
}
QTabWidget::pane {
		border-style: none;
}

QTabWidget ::pane {
	border-width: 0px;
	border: 0px;
}

QTabWidget QTabBar::tab {
	width: 150px;
    min-height: 28px;
    color: rgb(150,150, 150);
	background-color:rgb(64, 64, 64);
	text-align:left;
	font-size: 13px;
}

QTabWidget QTabBar::tab:selected {
	/*font: arial;*/
	color: rgb(225,150, 0);
	background-color:rgb(90, 85, 85);
	text-align:left;
	border-top: 1px solid rgb(225, 150, 0);
	margin-bottom: 1px;
}

QTabWidget QTabBar::tab:hover:!selected {
	/*font: arial;*/
	color: rgb(175, 175, 175);
	background-color: rgb(80, 75, 75);
}

QTabWidget QPushButton[cornerbutton= "true"]
{
    min-width: 28px;
    min-height: 28px;
    background-color:rgb(64, 64, 64);
    border: 1px solid rgb(64, 64, 64);
    border-radius: 4px;
    font-size:14px;
    qproperty-icon: url(resources/icons/add.svg);
}

QTabWidget QPushButton[cornerbutton= "true"]:hover:!pressed
{
    min-width: 28px;
    min-height: 28px;
	background-color: rgb(80, 75, 75);

}
QTabWidget QPushButton[cornerbutton= "true"]:pressed
{
    min-width: 28px;
    min-height: 28px;
	background-color:rgb(90, 85, 85);
    color:rgb(250, 175, 0);
}

QTabWidget QToolTip
{
    background-color:rgb(64,64,64);
    color:white;
}


QFrame[divider=true] {
	background-color: rgb(225,150, 0);
}

QComboBox {
	border: 0px solid gray;
	border-bottom:  2px solid rgb(45, 30, 30);
	border-radius: 0px;
	background-color: rgb(100, 95, 95);
	padding-left: 4px;
	font-size: 14px;
}

QComboBox[header=true] {
	border-bottom:  2px solid rgb(0, 200, 150);
}

QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 32px;
	border-width: 0px;
	border-radius: 0px;
}

QComboBox::down-arrow {
	image: url(D:/erdem/coding/Flow/resources/Laminar/icons/ic_keyboard_arrow_down_white_48dp_2x.png);
	height: 24px;
	width: 24px;
}

QComboBox::down-arrow:on {
	image: url(D:/erdem/coding/Flow/resources/Laminar/icons/ic_keyboard_arrow_up_white_48dp_2x.png);
}

QComboBox QAbstractItemView {
	border-width: 0px;
	outline: 0px;
	background-color: rgb(100, 100, 100);
	selection-background-color: rgb(150, 145, 145);
}

QListWidget {
	border-width: 0px;
	background-color: rgb(100,100,100);
	outline: 0px;
}

QListWidget:disabled,
